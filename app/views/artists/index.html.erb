<h1>Artists</h1>

<div id="artists">
  <% @artists.each do |artist| %>
    <%= render artist %>
    <p>
      <%= link_to "Show this artist", artist, class: 'btn btn-info' %>
    </p>
  <% end %>
  <%== raw pagy_bootstrap_nav(@pagy) if @pagy.pages > 1 %>
</div>

<%= link_to "New artist", new_artist_path, class: 'btn btn-success' %>
