<h1>Genders</h1>

<div id="genders">
  <% @genders.each do |gender| %>
    <%= render gender %>
    <p>
      <%= link_to "Show this gender", gender, class: 'btn btn-info' %>
    </p>
  <% end %>
  <%== raw pagy_bootstrap_nav(@pagy) if @pagy.pages > 1 %>
</div>

<%= link_to "New gender", new_gender_path, class: 'btn btn-success' %>
