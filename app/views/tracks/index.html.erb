<h1>Tracks</h1>

<div id="tracks">
  <% @tracks.each do |track| %>
    <%= render track %>
    <p>
      <%= link_to "Show this track", track, class: 'btn btn-info' %>
    </p>
  <% end %>

  <%== raw pagy_bootstrap_nav(@pagy) if @pagy.pages > 1 %>
</div>

<%= link_to "New track", new_track_path, class: 'btn btn-success' %>
